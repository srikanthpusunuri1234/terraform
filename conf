server {
    listen 80;
    server_name _;
    
    root /var/www/html;
    index index.html;
    try_files $uri /index.html;

    location /docs/ {
    alias /var/www/html/osm;
    index index.html;
    try_files $uri /index.html;

    auth_basic "Restricted Access";
    auth_basic_user_file /etc/nginx/.htpasswd;
   }

}
